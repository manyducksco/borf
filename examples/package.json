{
  "name": "@woofjs/examples",
  "version": "0.3.0",
  "description": "Woof development examples",
  "private": true,
  "source": "src/index.js",
  "main": "dist/woof.app.c.js",
  "module": "dist/woof.app.m.js",
  "types": "types.d.ts",
  "scripts": {
    "start": "woof-build -c client/client.js -o build --watch",
    "build": "woof-build -c client/client.js -o build --minify",
    "view": "woof-view start ./client --include-css client/styles/demo.css",
    "view:build": "woof-view build ./client --include-css client/styles/demo.css -o ./view-static",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "web app",
    "front end framework",
    "functional",
    "reactive state"
  ],
  "author": "Tony McCoy <tony@manyducks.co>",
  "license": "MIT",
  "devDependencies": {
    "@woofjs/build": "file:../packages/build",
    "@woofjs/client": "file:../packages/client",
    "@woofjs/view": "file:../packages/view"
  }
}
