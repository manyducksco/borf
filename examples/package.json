{
  "name": "@woofjs/examples",
  "version": "0.3.0",
  "description": "Woof development examples",
  "private": true,
  "type": "module",
  "scripts": {
    "start:prod": "node server/app.js",
    "start": "woof-build --watch",
    "build": "woof-build --minify --compress",
    "view": "woof-view start ./client",
    "view:build": "woof-view build ./client -o ./view-static",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "web app",
    "front end framework",
    "functional",
    "reactive state"
  ],
  "author": "Tony McCoy <tony@manyducks.co>",
  "license": "MIT",
  "devDependencies": {
    "woofe": "file:../packages/woofe",
    "@woofjs/build": "file:../packages/build",
    "@woofjs/data": "file:../packages/data",
    "@woofjs/server": "file:../packages/server",
    "@woofjs/view": "file:../packages/view",
    "popmotion": "^11.0.5",
    "stylefire": "^7.0.3"
  },
  "dependencies": {
    "ramda": "^0.28.0"
  }
}
